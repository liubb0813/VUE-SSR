{
  "name": "bk-tdsp-fe",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev:client": "vue-cli-service serve",
    "dev:serve": "cross-env NODE_ENV=development WEBPACK_TARGET=node nodemon ./server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:serve\" ",
    "build": "vue-cli-service build",
    "build:staging": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint",
    "build:client": "vue-cli-service build",
    "build:server": "cross-env WEBPACK_TARGET=node vue-cli-service build",
    "build:win": "npm run build:server && move dist\\vue-ssr-server-bundle.json bundle && npm run build:client && move bundle dist\\vue-ssr-server-bundle.json",
    "build:mac": "npm run build:server && mv dist/vue-ssr-server-bundle.json bundle && npm run build:client && mv bundle dist/vue-ssr-server-bundle.json",
    "start": "cross-env NODE_ENV=production WEBPACK_TARGET=node node ./server/server.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "express": "^4.16.4",
    "koa": "^2.7.0",
    "koa-router": "^7.4.0",
    "koa-send": "^5.0.0",
    "koa-static": "^5.0.0",
    "qs": "^6.7.0",
    "throttle-debounce": "^2.1.0",
    "tiny-cookie": "^2.3.1",
    "uuid": "^3.3.2",
    "vue": "^2.5.22",
    "vue-meta": "^1.5.8",
    "vue-router": "^3.0.2",
    "vue-server-renderer": "^2.5.22",
    "vuex": "^3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.3.0",
    "@vue/cli-plugin-eslint": "^3.3.0",
    "@vue/cli-service": "^3.3.1",
    "@vue/eslint-config-prettier": "^4.0.1",
    "async-validator": "^1.10.1",
    "babel-eslint": "^10.0.1",
    "concurrently": "^4.1.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.12.1",
    "eslint-plugin-vue": "^5.1.0",
    "lodash.merge": "^4.6.1",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.18.10",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "vue-template-compiler": "^2.5.22",
    "webpack": "^4.29.5"
  }
}
